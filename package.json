  },
  "resolutions": {
    "gatsby/eslint-plugin-import": "2.21.2",
    "gatsby/@typescript-eslint/eslint-plugin": "4.15.0"
  },
  "importSort": {
    ".tsx, .ts": {
      "style": "sova",
      "parser": "typescript"
    },
    ".js": {
      "style": "sova"
    }
  }
}
