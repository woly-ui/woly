{
  "name": "woly",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && node build",
    "lint": "yarn lint:code && yarn lint:style && yarn format",
    "lint:code": "eslint --fix --ext .ts,.tsx src",
    "lint:style": "stylelint src/**/*.{js,css,ts,tsx} --fix",
    "format": "prettier --write '{src,build,.storybook,packages}/**/**.{ts,tsx,js}'",
    "start": "gatsby develop",
    "docs:build": "gatsby build --prefix-paths",
    "test": "echo 'No tests provided'",
    "commit": "git-cz",
    "percy:cypress": "percy exec -- cypress run",
    "test:visual": "start-server-and-test 'yarn start' http://localhost:8000/package/woly/component/button 'yarn percy:cypress'"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
