    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
    "@babel/plugin-transform-react-constant-elements": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
