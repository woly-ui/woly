{
  "name": "woly",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && node build",
    "clean": "gatsby clean && rm -rf .temp",
    "commit": "git-cz",
    "docs:build": "yarn clean && gatsby build --prefix-paths",
    "format": "prettier --check '{src,build,.storybook,packages}/**/**.{ts,tsx,js,md,mdx,json}'",
    "format:fix": "prettier --write '{src,build,.storybook,packages}/**/**.{ts,tsx,js,md,mdx,json}'",
    "lint:code": "eslint --ext .ts,.tsx src",
    "lint:code:fix": "yarn lint:code --fix",
    "lint:style": "stylelint './src/**/*.{js,css,ts,tsx}'",
    "lint:style:fix": "stylelint './src/**/*.{js,css,ts,tsx}' --fix",
    "percy:cypress": "percy exec -- cypress run",
    "start": "yarn clean && gatsby develop",
    "test:screenshot": "node screenshot-testing",
    "percy:ci": "start-server-and-test 'yarn docs:build && gatsby serve -p 8000' 8000 'yarn percy exec yarn test:screenshot'",
    "test": "echo 'No tests provided'"
  },
  "devDependencies": {
