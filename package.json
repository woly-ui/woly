    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@testing-library/cypress": "^7.0.4",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/styled-components": "^5.1.7",
