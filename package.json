    "@eslint-kit/eslint-config-prettier": "^2.0.0",
    "@eslint-kit/eslint-config-react": "^2.1.0",
    "@eslint-kit/eslint-config-react-new-jsx-transform": "^1.0.0",
    "@eslint-kit/eslint-config-typescript": "^3.2.0",
    "@percy/cli": "^1.0.0-beta.57",
