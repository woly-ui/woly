{
  "presets": [["@babel/env"], "@babel/preset-react", "@babel/typescript"],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "~/": "./src",
          "~/ui": "./src/ui"
        }
      }
    ]
  ]
}
